package busReservation;

public class Bus {
	private int busno;
	private boolean ac;
	private int capacity;
	
	Bus(int no,boolean ac,int cap)
	{
		this.busno = no;
		this.ac = ac;
        this.capacity = cap;
	}
	public boolean isAc() {
		return ac;
	}
	
	public int getcapacity()
	{
		return capacity;
	}
	public void setAc(boolean val)
	{
		ac = val;
	}
	
	public void setcapacity(int cap)
	{
		capacity = cap;
	}
	public void displayBusinfo() {
		System.out.println("Enter busno :" + busno + "Ac :" + ac + "Total capacity" + capacity  );
	}
	public int getBusNo() {
		// TODO Auto-generated method stub
		return busno;
	}
}
